{
  "compilerOptions": {
  "allowSyntheticDefaultImports": true,
  "jsx": "react",
  "module": "commonjs",
  "noImplicitAny": true,
  "outDir": "./build/",
  "preserveConstEnums": true,
  "removeComments": true,
  "sourceMap": true,
  "noImplicitAny": false,
  "target": "es6",
  "lib": [
    "dom",
    "es6"
  ]
  },
  "include": [
    "./src/**/*",
    "typings/**/*.ts"
  ]
}
